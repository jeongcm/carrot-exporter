{
  "name": "nexclipper-api",
  "version": "0.9.15",
  "description": "TypeScript + Sequelize + MySQL + Express API Server",
  "author": "NexClipper Inc.",
  "license": "ISC",
  "scripts": {
    "start": "cross-env NODE_ENV=production node -r ./tsconfig-paths-bootstrap.js -r ./dist/src/monitoring/tracing.js ./dist/src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon server.js",
    "build": "tsc && npx tsc-alias",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": " jest  --forceExit --detectOpenHandles --coverage ",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "doc": "jsdoc -c jsdoc.json -r && node node_modules/serve/bin/serve.js ./jsdocs -p 30886",
    "deploy:prod": "npm run build && pm2 start ecosystem.config.js --only prod",
    "deploy:dev": "pm2 start ecosystem.config.js --only dev"
  },
